    "import torch as pt\n",
    "def tanh(x: pt.Tensor) -> pt.Tensor:\n",
    "    if type(x) is not pt.Tensor:\n",
    "        print(\"Not tensor\")\n",
    "        return\n",
    "    top = pt.exp(x)-pt.exp(-x)\n",
    "    bottem = pt.exp(x)+pt.exp(-x)\n",
    "    return top/bottem\n"
   ]
  }
